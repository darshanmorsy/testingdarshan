  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
  <script>
    function createPDF() {
      var element = document.getElementById('ddd');
      html2pdf(element, {
        margin: 1,
        padding: 0,
        filename: 'myfile.pdf',
        image: { type: 'jpeg', quality: 2080 },
        html2canvas: { scale: 2, logging: true },
        jsPDF: { unit: 'in', format: 'A3', orientation: 'P' },
        class: createPDF
      });
    };
