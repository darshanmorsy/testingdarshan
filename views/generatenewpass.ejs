<!-- [ auth-signin ] start -->

<div class="auth-wrapper">
    <div class="auth-content">
        <div class="card">
            <div class="row align-items-center text-center">
                <div class="col-md-12">
                    <div class="card-body">
                        <img src="/images/logo-dark.png" alt="" class="img-fluid mb-4">
                        <h4 class="mb-3 f-w-400">Signin</h4>

                        <form action="/newpass" method="post">
                            <div class="form-group mb-3">
                                <input type="text" name="npass" class="form-control p-3" id="npass"
                                    placeholder="enter password">
                            </div>
                            <div class="form-group mb-4">

                                <input type="password" name="cpass" class="form-control p-3" id="cpass"
                                    placeholder="confirm password">
                            </div>

                            <button class="btn btn-block btn-primary mb-4" type="submit">Generate New Password</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- [ auth-signin ] end -->

